{
  "name": "ai-exploratory-agent",
  "version": "1.0.0",
  "description": "AI-powered exploratory testing agent that autonomously explores web applications",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:tools": "jest --testPathPattern=tests/tools",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "validate:browser": "ts-node scripts/validate-browser.ts",
    "validate:tools": "ts-node scripts/validate-tools.ts",
    "validate:llm": "ts-node scripts/validate-llm.ts",
    "explore": "ts-node src/main.ts",
    "resume": "ts-node src/main.ts"
  },
  "keywords": [
    "testing",
    "automation",
    "ai",
    "exploratory-testing",
    "playwright"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.1",
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^16.4.1",
    "openai": "^4.28.0",
    "playwright": "^1.41.2",
    "zod": "^3.25.76"
  }
}